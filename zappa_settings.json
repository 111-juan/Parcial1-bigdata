{
    "dev": {
        "app_function": "app.f",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "parcial1-bigdat",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-parcial1bigdata",
        "apigateway_enabled": false,
        "keep_warm": false,
        "manage_roles": false,
        "role_name": "Labrole",
        "events": [
            
            {
                "function": "app.f",
                "expression": "cron(0 23 ? * * *)"
            }
            ]
    },
    "punto2": {
        "app_function": "puntodos.lambda_handler",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "basededatos-zappa2",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-parcial1bigdata",
        "apigateway_enabled": false,
        "keep_warm": false,
        "manage_roles": false,
        "role_name": "Labrole",
        "environment_variables": {
            "DB_HOST": "${DB_HOST}",
            "DB_USER": "${DB_USER}",
            "DB_PASS": "${DB_PASS}",
            "DB_NAME": "${DB_NAME}"
        },
        "events": [
            
            {
                "function": "puntodos.lambda_handler",
                "event_source": {
                    
                    "arn":"arn:aws:s3:::dolar-raw-parcial1bigdata",
                    "events":["s3:ObjectCreated:*"]
                    
                }
            }
            ]
    }
}